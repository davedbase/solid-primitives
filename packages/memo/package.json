{
  "name": "@solid-primitives/memo",
  "version": "1.3.9",
  "description": "Collection of custom memo primitives. They extend Solid's createMemo functionality while keeping the usage similar.",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://primitives.solidjs.community/package/memo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "memo",
    "stage": 3,
    "list": [
      "createLatest",
      "createLatestMany",
      "createWritableMemo",
      "createLazyMemo",
      "createPureReaction",
      "createMemoCache",
      "createReducer"
    ],
    "category": "Reactivity"
  },
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": {},
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsx ../../scripts/dev.ts",
    "build": "tsx ../../scripts/build.ts",
    "vitest": "vitest -c ../../configs/vitest.config.ts",
    "test": "pnpm run vitest",
    "test:ssr": "pnpm run vitest --mode ssr"
  },
  "keywords": [
    "solid",
    "primitives",
    "memo"
  ],
  "dependencies": {
    "@solid-primitives/scheduled": "workspace:^",
    "@solid-primitives/utils": "workspace:^"
  },
  "devDependencies": {
    "@solid-primitives/mouse": "workspace:^",
    "@solidjs/router": "^0.8.4",
    "solid-js": "^1.8.7"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "typesVersions": {}
}
