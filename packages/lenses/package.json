{
  "name": "@solid-primitives/lenses",
  "version": "0.0.100",
  "description": "Derived setters and getters for Stores.",
  "author": "Nathan Babcock <nathan.r.babcock@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/lenses#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "lenses",
    "stage": 0,
    "list": [
      "createLens",
      "createFocusedGetter",
      "createFocusedSetter"
    ],
    "category": "Utilities"
  },
  "keywords": [
    "solid",
    "primitives"
  ],
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "typesVersions": {},
  "scripts": {
    "dev": "jiti ../../scripts/dev.ts",
    "build": "jiti ../../scripts/build.ts",
    "vitest": "vitest -c ../../configs/vitest.config.ts",
    "test": "pnpm run vitest",
    "test:ssr": "pnpm run vitest --mode ssr",
    "typecheck": "pnpm run vitest typecheck"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  }
}
