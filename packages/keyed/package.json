{
  "name": "@solid-primitives/keyed",
  "version": "1.2.3",
  "description": "Control Flow primitives and components that require specifying explicit keys to identify or rerender elements.",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://primitives.solidjs.community/package/keyed",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "keyed",
    "stage": 3,
    "list": [
      "keyArray",
      "Key",
      "Entries"
    ],
    "category": "Control Flow"
  },
  "keywords": [
    "key",
    "control-flow",
    "solid",
    "primitives"
  ],
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsx ../../scripts/dev.ts",
    "build": "tsx ../../scripts/build.ts",
    "vitest": "vitest -c ../../configs/vitest.config.ts",
    "test": "pnpm run vitest",
    "test:ssr": "pnpm run vitest --mode ssr"
  },
  "devDependencies": {
    "@solid-primitives/refs": "workspace:^",
    "@solid-primitives/utils": "workspace:^",
    "solid-js": "^1.8.7",
    "solid-transition-group": "^0.2.3"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "typesVersions": {}
}
