{
  "name": "@solid-primitives/deep",
  "version": "0.2.10",
  "description": "Primitives for tracking and observing nested reactive objects in Solid.",
  "author": "Samuel Burbano <me@iosamuel.dev>",
  "contributors": [
    "Damian Tarnawski <gthetarnav@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://primitives.solidjs.community/package/deep",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "deep",
    "stage": 1,
    "list": [
      "trackDeep",
      "trackStore",
      "captureStoreUpdates"
    ],
    "category": "Reactivity"
  },
  "keywords": [
    "solid",
    "primitives",
    "deep",
    "store",
    "reactivity",
    "delta"
  ],
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": {},
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "typesVersions": {},
  "scripts": {
    "dev": "tsx ../../scripts/dev.ts",
    "build": "tsx ../../scripts/build.ts",
    "vitest": "vitest -c ../../configs/vitest.config.ts",
    "test": "pnpm run vitest",
    "test:ssr": "pnpm run vitest --mode ssr"
  },
  "dependencies": {
    "@solid-primitives/memo": "workspace:^"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "devDependencies": {
    "solid-js": "^1.8.7"
  }
}
