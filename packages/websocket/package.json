{
  "name": "@solid-primitives/websocket",
  "version": "0.4.0",
  "description": "Primitive to create a web socket connection",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "contributors": [
    "Alex Lohr <alex.lohr@logmein.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/websocket",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "websocket",
    "stage": 0,
    "list": [
      "makeWS",
      "createWS",
      "makeReconnectingWS",
      "createReconnectingWS"
    ],
    "category": "network"
  },
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": {},
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "jiti ../../scripts/build.ts",
    "test": "vitest"
  },
  "keywords": [
    "websocket",
    "socket",
    "solid",
    "primitives"
  ],
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "typesVersions": {}
}
