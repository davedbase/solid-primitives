  "devDependencies": {
    "jsdom": "^18.1.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "solid-register": "^0.0.18",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2",
    "uvu": "^0.5.2",
    "unbuild": "^0.5.13"
  },
  "peerDependencies": {
    "solid-js": "^1.2.5"
  }
}