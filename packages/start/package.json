{
  "name": "@solid-primitives/start",
  "version": "0.0.1",
  "description": "A set of primitives for Solid Start",
  "author": "Thomas",
  "contributors": [
    "Damian Tarnawski <gthetarnav@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/start#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "start",
    "stage": 0,
    "list": [
      "createServerCookie",
      "createUserTheme"
    ],
    "category": "Solid Start"
  },
  "keywords": [
    "solid",
    "primitives",
    "solid-start",
    "ssr"
  ],
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "typesVersions": {},
  "scripts": {
    "dev": "jiti ../../scripts/dev.ts",
    "build": "jiti ../../scripts/build.ts",
    "vitest": "vitest -c ../../configs/vitest.config.ts",
    "test": "pnpm run vitest",
    "test:ssr": "pnpm run vitest --mode ssr"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12",
    "solid-start": "^0.2.26"
  },
  "devDependencies": {
    "solid-js": "1.7.3"
  }
}
