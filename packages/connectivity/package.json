{
  "name": "@solid-primitives/connectivity",
  "version": "0.3.20",
  "description": "A navigator.onLine signal.",
  "author": "Klemen Slaviƒç <krof.drakula@gmail.com>",
  "contributors": [
    "Damian Tarnawski <gthetarnav@gmail.com>",
    "Tom Pichaud <dev.tompichaud@icloud.com>"
  ],
  "license": "MIT",
  "homepage": "https://primitives.solidjs.community/package/connectivity",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "connectivity",
    "stage": 2,
    "list": [
      "createConnectivitySignal"
    ],
    "category": "Network"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsx ../../scripts/dev.ts",
    "build": "tsx ../../scripts/build.ts",
    "vitest": "vitest -c ../../configs/vitest.config.ts",
    "test": "pnpm run vitest",
    "test:ssr": "pnpm run vitest --mode ssr"
  },
  "keywords": [
    "network",
    "online",
    "solid",
    "primitives"
  ],
  "dependencies": {
    "@solid-primitives/event-listener": "workspace:^",
    "@solid-primitives/rootless": "workspace:^",
    "@solid-primitives/utils": "workspace:^"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "devDependencies": {
    "solid-js": "^1.8.7"
  },
  "typesVersions": {}
