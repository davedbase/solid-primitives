{
  "name": "@solid-primitives/timers",
  "version": "0.0.100",
  "description": "Primitives related to setTimeout and setInterval",
  "author": "Xavier Loh <xavier.loh.yz@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/timers#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "timers",
    "stage": 0,
    "list": [
      "createBasicTimeout",
      "createBasicInterval",
      "createTimeout",
      "createInterval",
      "createTimeoutLoop",
      "createPolled",
      "createIntervalCounter"
    ],
    "category": "Browser APIs"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "yarn start",
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "setInterval",
    "setTimeout",
    "timer",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "prettier": "^2.5.1",
    "solid-register": "^0.1.5",
    "tslib": "^2.3.1",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2",
    "unocss": "0.28.1",
    "uvu": "^0.5.3",
    "vite": "2.8.6",
    "vite-plugin-solid": "2.2.6"
  },
  "peerDependencies": {
    "solid-js": "^1.3.0"
  },
  "solid-register": {
    "dom": false
  }
}
