{
  "name": "@solid-primitives/scheduled",
  "version": "1.3.2",
  "description": "Primitives for creating scheduled — throttled or debounced — callbacks.",
  "contributors": [
    "David Di Biase <dave.dibiase@gmail.com>",
    "Damian Tarnawski <gthetarnav@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/scheduled#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "scheduled",
    "stage": 3,
    "list": [
      "debounce",
      "throttle",
      "scheduleIdle",
      "leading",
      "createScheduled"
    ],
    "category": "Utilities"
  },
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "dev": "jiti ../../scripts/dev.ts",
    "build": "jiti ../../scripts/build.ts",
    "vitest": "vitest -c ../../configs/vitest.config.ts",
    "test": "pnpm run vitest",
    "test:ssr": "pnpm run vitest --mode ssr"
  },
  "keywords": [
    "solid",
    "primitives",
    "scheduled",
    "debounce",
    "throttle"
  ],
  "devDependencies": {
    "@solid-primitives/timer": "workspace:^",
    "@solidjs/router": "^0.8.2",
    "solid-js": "1.7.3"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "typesVersions": {}
}
