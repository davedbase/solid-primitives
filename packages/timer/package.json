{
  "name": "@solid-primitives/timer",
  "version": "1.3.7",
  "description": "Primitives to manage timeout and interval",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "contributors": [
    "Xavier Loh <xavier.loh.yz@gmail.com>",
    "Damian Tarnawski <gthetarnav@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/timer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "timer",
    "stage": 3,
    "list": [
      "makeTimer",
      "createTimer",
      "createTimeoutLoop",
      "createPolled",
      "createIntervalCounter"
    ],
    "category": "Browser APIs"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": {},
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "jiti ../../scripts/dev.ts",
    "build": "jiti ../../scripts/build.ts",
    "vitest": "vitest -c ../../configs/vitest.config.ts",
    "test": "pnpm run vitest",
    "test:ssr": "pnpm run vitest --mode ssr"
  },
  "keywords": [
    "setInterval",
    "setTimeout",
    "timer",
    "solid",
    "primitives"
  ],
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "typesVersions": {},
  "devDependencies": {
    "solid-js": "1.7.5"
  }
}
