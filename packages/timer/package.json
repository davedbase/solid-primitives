{
  "name": "@solid-primitives/timer",
  "version": "1.1.0",
  "description": "Primitive to manage timeout and interval",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/timer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "timer",
    "stage": 3,
    "list": [
      "createTimer",
      "createPolled"
    ],
    "category": "Utilities"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "setInterval",
    "setTimeout",
    "timer",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "@types/jest": "^27.0.0",
    "prettier": "^2.0.5",
    "solid-register": "^0.2.3",
    "tslib": "^2.0.1",
    "tsup": "^5.10.1",
    "typescript": "^4.0.2",
    "uvu": "^0.5.3"
  },
  "peerDependencies": {
    "solid-js": "^1.3.1"
  },
  "solid-register": {
    "dom": false
  }
}
